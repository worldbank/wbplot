{
    "font": {
        "fontFamily": "Arial",
        "fontWeight": {
            "regular": 400,
            "semibold": 600,
            "bold": 700
        }
    },
    "chartSmall": {
        "fontSize": {
            "s": 12,
            "m": 14,
            "l": 16
        },
        "spacing": {
            "xxs": 2,
            "xs": 4,
            "s": 6,
            "m": 12,
            "l": 14,
            "xl": 16
        }
    },
    "chartMedium": {
        "fontSize": {
            "s": 13,
            "m": 15,
            "l": 18
        },
        "spacing": {
            "xxs": 3,
            "xs": 6,
            "s": 9,
            "m": 15,
            "l": 18,
            "xl": 21
        }
    },
    "chartLarge": {
        "fontSize": {
            "s": 14,
            "m": 16,
            "l": 20
        },
        "spacing": {
            "xxs": 4,
            "xs": 8,
            "s": 12,
            "m": 16,
            "l": 20,
            "xl": 24
        }
    },
    "chartTitle": {
        "size": "l",
        "weight": "bold",
        "height": "120",
        "color": "darkText"
    },
    "subTitle": {
        "size": "m",
        "weight": "regular",
        "height": "120",
        "color": "lightText"
    },
    "noteTitle": {
        "size": "s",
        "weight": "bold",
        "height": "150",
        "color": "darkText"
    },
    "note": {
        "size": "s",
        "weight": "regular",
        "height": "150",
        "color": "lightText"
    },
    "axisLabel": {
        "size": "m",
        "weight": "semibold",
        "height": "120",
        "color": "darkText"
    },
    "tickLabel": {
        "size": "s",
        "weight": "regular",
        "height": "120",
        "color": "lightText"
    },
    "gridLine": {
        "linewidth": 1,
        "color": "middle",
        "dash": "4 2"
    },
    "zeroLine": {
        "zeroLine": 1,
        "color": "lighter"
    }
}
